{
  "extends": [
    "standard",
    "plugin:flowtype/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "jsdoc",
    "flowtype"
  ],
  "rules": {
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "functions": "never"
    }],
    "no-multiple-empty-lines": ["error", {
      "max": 2
    }]
  }
}
